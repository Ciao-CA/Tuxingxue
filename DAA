% --- Executes on button press in pushbutton1.
function pushbutton1_Callback(hObject, eventdata, handles)
% hObject    handle to pushbutton1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
cla(handles.axes1);
x1=str2double(get(handles.edit1,'String'));
x2=str2double(get(handles.edit2,'String'));
y1=str2double(get(handles.edit3,'String'));
y2=str2double(get(handles.edit4,'String'));
color=get(handles.edit16,'String');

X=[];
Y=[];
if(abs(x2-x1)>=abs(y2-y1))
    dm=abs(x2-x1);
else
    dm=abs(y2-y1);
end
dx=(x2-x1)/dm;
dy=(y2-y1)/dm;
x=x1;
y=y1;
for i=1:dm
    X(i)=x;
    Y(i)=y;
    x=x+dx;
    y=y+dy;
end


for i=1:dm
    axes(handles.axes1);
    plot(X(:,1:i),Y(:,1:i),color);
    hold on;
    pause(0.1);
    axis([x1,x2,y1,y2]);
    grid on;
end
set(handles.axes1,'Xminortick','on');
pause(0.2)
axis([-100,100,-100,100]);
% set(handles.axe1,'position',[0,100,0,100]);
grid on;



