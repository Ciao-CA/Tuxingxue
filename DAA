% --- Executes on button press in pushbutton2.
function pushbutton2_Callback(hObject, eventdata, handles)
% hObject    handle to pushbutton2 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
cla(handles.axes1);
x1=str2double(get(handles.edit5,'String'));
x2=str2double(get(handles.edit6,'String'));
y1=str2double(get(handles.edit7,'String'));
y2=str2double(get(handles.edit8,'String'));
color=get(handles.edit17,'String');
dx=abs(x2-x1);
dy=abs(y2-y1);
flag=0;
if(dx<dy)
    flag=1;
    t=x1;x1=y1;y1=t;
    t=x2;x2=y2;y2=t;
    t=dx;dx=dy;dy=t;
end
if(x2-x1>0)
    tx=1;
else
    tx=-1;
end
if(y2-y1>0)
    ty=1;
else
    ty=-1;
end
curx=x1;cury=y1;
dS=2*dy;
dT=2*(dy-dx);
d=dS-dx;i=1;
while curx~=x2
    if(d<0)
        d=dS+1;
    else
        cury=cury+ty;
        d=d+dT;
    end
    if(flag)
        X(i)=cury;
        Y(i)=curx;
    else
        X(i)=curx;
        Y(i)=cury;
    end
    curx=curx+tx;i=i+1;
end
axes(handles.axes1);
for j=1:i-1
    plot(X(:,1:j),Y(:,1:j),color);
    pause(0.1);
    hold on;
    axis([x1,x2,y1,y2]);
    grid on;
end
pause(0.2);
axis([-100,100,-100,100]);
